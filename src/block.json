{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dblocks/hotspot",
	"version": "0.1.0",
	"title": "DBlocks Hotspot",
	"category": "widgets",
	"icon": "marker",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": true,
		"color": true,
		"typography": true,
		"align": [
			"wide",
			"full"
		]
	},
	"attributes": {
		"hotspotNumbers": {
			"type": "array",
			"default": []
		},
		"startNumber": {
			"type": "number",
			"default": 1
		},
		"hotspotBackgroundColor": {
			"type": "string",
			"default": "#8d57fd"
		},
		"hotspotTextColor": {
			"type": "string",
			"default": "#FFFFFF"
		},
		"hotspotFontSize": {
			"type": "string",
			"default": "16px"
		}
	},
	"textdomain": "dblocks-hotspot",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}